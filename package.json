{
  "name": "livus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "test": "node --expose-gc ./node_modules/.bin/jest --maxWorkers=50% --logHeapUsage --no-cache -c jest.config.js",
    "test:integration": "node --expose-gc ./node_modules/.bin/jest --maxWorkers=50% --logHeapUsage --no-cache -c jest.config.integration.js",
    "test:reset": "rm -rf node_modules yarn.lock package-lock.json coverage && yarn",
    "cypress:open": "TZ=UTC cypress open",
    "cypress:run": "TZ=UTC cypress run"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "17.0.13",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "babel-jest": "^27.0.6",
    "cypress": "^7.6.0",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5",
    "eslint-plugin-prettier": "^3.4.0"
  }
}
